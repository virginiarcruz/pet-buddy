{
  "presets": ["next/babel", "@babel/preset-typescript"],
  "plugins": [
    [
      "babel-plugin-styled-components",
      {
        "ssr": true,
        "displayName": true
      }
    ]
  ],
  "env": {
    "test": {
      "plugins": [
        [
          "babel-plugin-styled-components",
          {
            "ssr": false,
            "displayName": false
          }
        ],
        [
          "inline-react-svg",
          {
            "svgo": {
              "plugins": [
                {
                  "name": "removeAttrs",
                  "params": { "attrs": "(data-name)" }
                },
                "cleanupIDs"
              ]
            }
          }
        ]
      ]
    }
  }
}
